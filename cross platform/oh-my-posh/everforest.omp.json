{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "background": "#A7C080",
                    "foreground": "#272E33",
                    "leading_diamond": "\ue0b6",
                    "properties": {
                        "style": "folder"
                    },
                    "style": "diamond",
                    "template": " \ue5ff {{ .Path }} ",
                    "trailing_diamond": "\ue0b0",
                    "type": "path"
                },
                {
                    "background": "#7FBBB3",
                    "foreground": "#272E33",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "fetch_stash_count": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "powerline",
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
                    "type": "git"
                },
                {
                    "background": "#E67E80",
                    "foreground": "#272E33",
                    "powerline_symbol": "\ue0b0",
                    "style": "powerline",
                    "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
                    "type": "python"
                },
                {
                    "background": "#E67E80",
                    "foreground": "#272E33",
                    "powerline_symbol": "\ue0b0",
                    "style": "powerline",
                    "template": " \uf0e7 ",
                    "type": "root"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}